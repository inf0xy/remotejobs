{
  "compilerOptions": {
    "jsx": "react",
    "module": "CommonJS",
    "target": "es5",
    "rootDir": "./",
    "outDir": "./build",
    "moduleResolution": "node",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  }
}